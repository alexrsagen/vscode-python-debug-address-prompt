{
  "name": "python-debug-address-prompt",
  "displayName": "Python Debugging Address Prompt",
  "description": "Prompt for host/port before starting a Python debugging session",
  "version": "1.0.0",
  "publisher": "alexrsagen",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexrsagen/vscode-debug-address-prompt"
  },
  "bugs": {
    "url": "https://github.com/alexrsagen/vscode-debug-address-prompt/issues"
  },
  "engines": {
    "vscode": "^1.33.1"
  },
  "categories": [
    "Debuggers"
  ],
  "activationEvents": [
    "onDebug"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "vscode": "^1.1.33"
  }
}
